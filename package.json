{
  "name": "unqueue-cli",
  "version": "1.0.14",
  "description": "Utilities CLI tool for ramping up development",
  "main": "./dist/index.js",
  "types": "./dist/unqueue-cli.d.ts",
  "bin": {
    "unq": "./dist/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "esbuild src/index.ts --bundle --outdir=dist --platform=node --minify && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.json --outFile ./dist/unqueue-cli.d.ts",
    "build:watch": "esbuild src/index.ts --bundle --outdir=dist --platform=node --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/UncleKhab/unqueue_cli.git"
  },
  "keywords": [
    "react",
    "cli",
    "tool",
    "clitool",
    "typescript"
  ],
  "author": "Stefan Cojocaru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UncleKhab/unqueue_cli/issues"
  },
  "homepage": "https://github.com/UncleKhab/unqueue_cli#readme",
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/prettier": "^2.7.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.1",
    "commander": "^10.0.0",
    "esbuild": "^0.17.7",
    "fs-extra": "^11.1.0",
    "inquirer": "^9.1.4",
    "prettier": "^2.8.4",
    "unqueue-cli": "^1.0.14",
    "util": "^0.12.5"
  }
}
